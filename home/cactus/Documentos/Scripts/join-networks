#!/bin/bash

networks=( 285-751-906 243-168-934 299-495-003 282-194-076 328-281-901 242-604-603 245-606-316 284-203-805 328-281-736 
328-281-998 288-350-932 270-487-290 299-639-152 249-798-746 335-416-084 333-233-282 322-559-720 284-684-655 352-092-274 
323-533-996 335-157-264 334-264-557 279-852-435 291-729-775 288-400-590 287-510-673 289-595-280 321-840-612 345-813-228 
308-523-916 322-834-533 336-707-134 347-989-120 319-546-327 352-684-120 316-539-687 375-496-747 )

echo "Enter the password, then press [ENTER]: "

read password

for network in "${networks[@]}"
do
  hamachi do-join "${network}" "${password}"
  sleep 5
done

exit 0
